generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Ping {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
}

model User{
  id  String   @id @default(cuid())
  first_name String
  last_name String
  password String
  email String @unique
  createdAT DateTime @default(now())
}